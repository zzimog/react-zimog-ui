{
  "name": "react-zimog-ui",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "shiki": "^3.22.0"
  },
  "peerDependencies": {
    "@fontsource-variable/manrope": "^5.2.8",
    "@tailwindcss/vite": "^4.1.13",
    "clsx": "^2.1.1",
    "lucide-react": "^0.561.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.1",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react-router": "^7.13.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^8.0.0-beta.13"
  },
  "overrides": {
    "vite": "^8.0.0-beta.13"
  },
  "prettier": {
    "endOfLine": "lf",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "importOrder": [
      "^(react/(.*)$)|^(react$)",
      "<THIRD_PARTY_MODULES>",
      "^@ui$",
      "^@ui/headless(.*)$",
      "^@ui/components(.*)$",
      "^@ui/hooks(.*)$",
      "^@ui/utils(.*)$",
      "^\\.\\./(.*)$",
      "^\\./(?!context|classes)(.*)$",
      "^\\./context$",
      "^\\./classes$"
    ],
    "importOrderParserPlugins": [
      "typescript",
      "jsx"
    ],
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ]
  }
}
